{
  "name": "anchor-js",
  "version": "0.4.0",
  "author": "bryanbraun (https://github.com/bryanbraun)",
  "description": "A tiny javscript utility for adding anchor links to existing page content.",
  "homepage": "https://github.com/bryanbraun/anchorjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/bryanbraun/anchorjs.git"
  },
  "bugs": {
    "url": "https://github.com/bryanbraun/anchorjs/issues"
  },
  "license": "MIT",
  "scripts": {
    "lint": "eslint anchor.js",
    "jasmine": "node_modules/karma/bin/karma start test/config.js --single-run",
    "release": "uglifyjs anchor.js --compress --mangle --screw-ie8 --comments \"/Copyright/\" -o anchor.min.js",
    "test": "npm run lint && npm run jasmine"
  },
  "devDependencies": {
    "eslint": "~0.19.0",
    "uglify-js": "~2.4.16",
    "karma": "0.12.31",
    "karma-jasmine": "^0.3.5",
    "karma-phantomjs-launcher": "^0.1.4",
    "jasmine-core": "~2.2.0"
  },
  "engines": {
    "node": ">=0.10.1"
  }
}
